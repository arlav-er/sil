<pretty-config xmlns="http://ocpsoft.org/prettyfaces/3.3.3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
               http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="registerCitt">
		<pattern value="/register/cittadino" />
		<view-id value="/public/register/cittadino/main.xhtml" />
	</url-mapping>

	<url-mapping id="registerAzienda">
		<pattern value="/register/azienda" />
		<view-id value="/public/register/azienda/main.xhtml" />
	</url-mapping>

	<url-mapping id="confirmRegister">
		<pattern value="/register/confirm/#{username}/#{email}/#{token}" />
		<view-id value="/public/register/confirm/main.xhtml" />
	</url-mapping>

	<url-mapping id="registerSuccess">
		<pattern value="/register/success/#{email}" />
		<view-id value="/public/register/success.xhtml" />
	</url-mapping>

	<url-mapping id="forgotPassword">
		<pattern value="/forgotPassword" />
		<view-id value="/public/forgotPassword/main.xhtml" />
	</url-mapping>

	<url-mapping id="changePassword">
		<pattern value="/changePassword/#{username}/#{email}/#{token}" />
		<view-id value="/public/changePassword/main.xhtml" />
	</url-mapping>

	<url-mapping id="sessionChangePassword">
		<pattern value="/secure/changePassword" />
		<view-id value="/secure/profile/changePassword/main.xhtml" />
	</url-mapping>

	<url-mapping id="profile">
		<pattern value="/secure/account" />
		<view-id value="/secure/profile/viewMain.xhtml" />
	</url-mapping>

	<url-mapping id="editProfile">
		<pattern value="/secure/account/edit" />
		<view-id value="/secure/profile/editMain.xhtml" />
	</url-mapping>

	<url-mapping id="tokens">
		<pattern value="/secure/account/tokens" />
		<view-id value="/secure/profile/tokensMain.xhtml" />
	</url-mapping>

	<url-mapping id="badges">
		<pattern value="/secure/account/badges" />
		<view-id value="/secure/profile/badgesMain.xhtml" />
	</url-mapping>

	<url-mapping id="provinciaEditAzienda">
		<pattern value="/secure/azienda/edit/#{aziendaId}" />
		<view-id value="/secure/profile/provincia/editAzienda.xhtml" />
	</url-mapping>

	<url-mapping id="provinciaViewAzienda">
		<pattern value="/secure/azienda/view/#{aziendaViewId}" />
		<view-id value="/secure/profile/provincia/viewAzienda.xhtml" />
	</url-mapping>
	
	<url-mapping id="provinciaCreaCittadino">
		<pattern value="/secure/cittadino/create" />
		<view-id value="/secure/profile/provincia/creaCittadino.xhtml" />
	</url-mapping>

	<url-mapping id="abilitaUtentiSare">
		<pattern value="/secure/sare/utenti/abilita" />
		<view-id value="/secure/profile/provincia/sare/abilita/main.xhtml" />
	</url-mapping>

	<url-mapping id="validaUtentiSare">
		<pattern value="/secure/azienda/utenti/valida" />
		<view-id value="/secure/profile/provincia/sare/valida/main.xhtml" />
	</url-mapping>

	<url-mapping id="accreditamentoForteUtenti">
		<pattern value="/secure/accreditamentoForte" />
		<view-id value="/secure/profile/provincia/accreditamentoForte/main.xhtml" />
	</url-mapping>

	<url-mapping id="accreditamentoForteViewUtente">
		<pattern value="/secure/accreditamentoForte/view/#{utenteId}" />
		<view-id value="/secure/profile/provincia/accreditamentoForte/viewUtente.xhtml" />
	</url-mapping>

	<url-mapping id="accreditamentoForteEditUtente">
		<pattern value="/secure/accreditamentoForte/edit/#{utenteId}" />
		<view-id value="/secure/profile/provincia/accreditamentoForte/editUtente.xhtml" />
	</url-mapping>

	<url-mapping id="jamon">
		<pattern value="/secure/jamon" />
		<view-id value="/secure/administration/jamon.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilatura">
		<pattern value="/secure/gestioneProfilatura" />
		<view-id value="/secure/administration/profilatura/utenti/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaCreaUtenti">
		<pattern value="/secure/gestioneProfilatura/creaUtenti" />
		<view-id value="/secure/administration/profilatura/creaUtenti/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaGruppi">
		<pattern value="/secure/gestioneProfilatura/gruppi" />
		<view-id value="/secure/administration/profilatura/gruppi/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaRuoli">
		<pattern value="/secure/gestioneProfilatura/ruoli" />
		<view-id value="/secure/administration/profilatura/ruoli/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaUtenti">
		<pattern value="/secure/gestioneProfilatura/utenti" />
		<view-id value="/secure/administration/profilatura/utenti/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaRuoliGruppi">
		<pattern value="/secure/gestioneProfilatura/ruoliGruppi" />
		<view-id value="/secure/administration/profilatura/ruoliGruppi/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaComponenti">
		<pattern value="/secure/gestioneProfilatura/componenti" />
		<view-id value="/secure/administration/profilatura/componenti/main.xhtml" />
	</url-mapping>

	<url-mapping id="gestioneProfilaturaNuovoSare">
		<pattern value="/secure/gestioneProfilatura/nuovoSare" />
		<view-id value="/secure/administration/profilatura/nuovoSare/main.xhtml" />
	</url-mapping>
	
	<url-mapping id="gestioneProfilaturaStatistiche">
		<pattern value="/secure/gestioneProfilatura/statistiche" />
		<view-id value="/secure/administration/profilatura/statistiche/main.xhtml" />
	</url-mapping>
</pretty-config>