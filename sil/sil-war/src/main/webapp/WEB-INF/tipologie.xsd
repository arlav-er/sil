<?xml version="1.0" encoding="iso-8859-1"?><xsd:schema	xmlns="http://www.w3.org/2001/XMLSchema"	targetNamespace="https://sifer.regione.emilia-romagna.it/WebService/Crisi/tipologie"	xmlns:xsd="http://www.w3.org/2001/XMLSchema"	elementFormDefault="qualified">		<xsd:simpleType name="varchar_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\S{1}[\S| ]{0,253}\S{1}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="sesso_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="(F|M)"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="date_type">		<xsd:restriction base="xsd:date">			<xsd:pattern value="\d{4}-\d{2}-\d{2}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="codice_fiscale_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="([A-Z]{6}[\dLMNPQRSTUV]{2}[A-Z][\dLMNPQRSTUV]{2}[A-Z][\dLMNPQRSTUV]{3}[A-Z])|(\d{11})"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="codice_catastale_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="[A-Z]\d{3}"/>			<xsd:pattern value="NT\s\s"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="tipo_contratto_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="[A-Z]\.\d{2}\.\d{2}"/>			<xsd:pattern value="NT"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="codice_istat_nazione_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\d{3}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="cap_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\d{5}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="email_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="(([A-Za-z0-9]+_+)|([A-Za-z0-9]+\-+)|([A-Za-z0-9]+\.+)|([A-Za-z0-9]+\++))*[A-Za-z0-9]+@((\w+\-+)|(\w+\.))*\w{1,63}\.[a-zA-Z]{2,6}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="si_no_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="(S|N)"/>		</xsd:restriction>	</xsd:simpleType>						<xsd:simpleType name="rif_pa_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="(200[789]|201[0123])-\d+/(RER|BO|FE|FC|MO|PR|PC|RA|RE|RN)(/\d+)?"/>		</xsd:restriction>	</xsd:simpleType>	</xsd:schema>