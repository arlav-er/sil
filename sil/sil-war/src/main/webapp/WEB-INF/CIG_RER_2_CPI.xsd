                <xs:annotation>
                    <xs:documentation>Codice originario della domanda CIG che si sta prorogando</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
