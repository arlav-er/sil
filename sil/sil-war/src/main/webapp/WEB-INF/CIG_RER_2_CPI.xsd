                <xs:annotation>
                    <xs:documentation>Indica se si tratta di impresa artigiana</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
