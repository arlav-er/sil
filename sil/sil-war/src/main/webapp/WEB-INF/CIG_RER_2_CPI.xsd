    <xs:complexType name="Rel_Accordo_UA_Lavoratore">
        <xs:annotation>
            <xs:documentation>Rappresenta CI_REL_ACCORDO_UNIT_AZ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lavoratori" type="ci:TipoLavoratore" maxOccurs="unbounded"/>
            <xs:element name="sede" type="ci:TipoSede"/>
        </xs:sequence>
        <xs:attribute name="flgCoinvolta" use="required">
            <xs:annotation>
                <xs:documentation>Indica se l'unità aziendale è coinvolta nell'accordo. (S/N/null)
L'essere non coinvolta (flag='N') significa che nel quadro D l'azienda è stata indicata come non coinvolta nella CIGS in deroga.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="1"/>
                    <xs:enumeration value="S"/>
                    <xs:enumeration value="N"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="numLavForza" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="0"/>
                    <xs:totalDigits value="38"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="strNumeroINPS"/>
        <xs:attribute name="strSedeINPS"/>
        <xs:attribute name="codAzTipo">
            <xs:annotation>
                <xs:documentation>Tipo di azienda per CIG RER. Nello schema da SARE si chiamava: macroSettore</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="strOrgDatoriale">
            <xs:annotation>
                <xs:documentation>CIGS: organizzazione sindacale a cui è associata l'impresa</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="100"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="strConsulenteLav">
            <xs:annotation>
                <xs:documentation>Professionista o consulente del lavoro</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="80"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TipoAccordoCIG">
        <xs:sequence>
            <xs:element name="datiAccordo" type="ci:DatiAccordo"/>
            <xs:element name="esameCongiunto" type="ci:TipoEsameCongiunto" minOccurs="0"/>
            <xs:element name="azienda" type="ci:TipoTestataAzienda"/>
            <xs:element name="relAzienda" type="ci:Rel_Accordo_UA_Lavoratore" maxOccurs="unbounded"/>
            <xs:element name="sindacati" type="ci:TipoSindacato" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="schemaVersion" type="xs:integer" use="required" fixed="1"/>
    </xs:complexType>
    <xs:element name="accordoCIG" type="ci:TipoAccordoCIG"/>
</xs:schema>
