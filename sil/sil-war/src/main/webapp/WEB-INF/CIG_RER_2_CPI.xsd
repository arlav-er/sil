                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="numVersione" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Versione (interna) del tracciato</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
