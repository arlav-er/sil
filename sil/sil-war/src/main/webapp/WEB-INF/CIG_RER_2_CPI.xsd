                    <xs:documentation>indica se richiesto pagamento diretto dei lavoratori. Flag S/N/null</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="datConsultaSind" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>numero di lavoratori in forza all'inizio del periodo di riferimento (complessivamente su tutte le sedi)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numLavForzaCompl" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>numero di lavoratori in forza all'inizio del periodo di riferimento (complessivamente su tutte le sedi)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="38"/>
                        <xs:fractionDigits value="0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="strCognomeRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>cognome del referente dell'accordo</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="40"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="strNomeRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>nome del referente dell'accordo</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="40"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="strTelRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>telefono del referente</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="strFaxRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>fax del referente</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="strEmailRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>E-mail del referente</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="80"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
