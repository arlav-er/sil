                <xs:annotation>
                    <xs:documentation>nome del legale rappresentante</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="40"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="flgPagDiretto" minOccurs="0">
