<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.satanet.it/Madreperla-SIL" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:tns="http://www.satanet.it/Madreperla-SIL" xmlns:Q1="caregiver">

  <xsd:element name="getCareGiver_input" type="tns:getCareGiver_inputType"></xsd:element>

  <xsd:complexType name="getCareGiver_inputType">
    <xsd:sequence>
      <xsd:element name="codiceFiscale" nillable="false">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="50"></xsd:maxLength>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>