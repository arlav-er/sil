<?xml version="1.0" encoding="UTF-8"?><xsd:schema	xmlns="http://www.w3.org/2001/XMLSchema"	targetNamespace="https://sifer.regione.emilia-romagna.it/WebService/Tirocini/tipo"    xmlns:tns="https://sifer.regione.emilia-romagna.it/WebService/Tirocini/tipo"	xmlns:xsd="http://www.w3.org/2001/XMLSchema"	elementFormDefault="qualified"    version="1.0">    <xsd:simpleType name="codice_fiscale_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="([A-Z]{6}[\dLMNPQRSTUV]{2}[A-Z][\dLMNPQRSTUV]{2}[A-Z][\dLMNPQRSTUV]{3}[A-Z])|(\d{11})"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="codice_provincia_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{3}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="codice_catastale_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z]\d{3}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="sesso_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="F|M"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="date_type">        <xsd:restriction base="xsd:date">            <xsd:pattern value="\d{4}-(0[1-9]|1[0-2])-(0[1-9]|[1-2][0-9]|3[0-1])"/>        </xsd:restriction>    </xsd:simpleType>		<xsd:simpleType name="codice_istat_nazione_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\d{3}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="cap_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="\d{5}"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="email_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="(([A-Za-z0-9]+_+)|([A-Za-z0-9]+\-+)|([A-Za-z0-9]+\.+)|([A-Za-z0-9]+\++))*[A-Za-z0-9]+@((\w+\-+)|(\w+\.))*\w{1,63}\.[a-zA-Z]{2,6}"/>            <xsd:maxLength value= "80"/>		</xsd:restriction>	</xsd:simpleType>		<xsd:simpleType name="si_no_type">		<xsd:restriction base="xsd:string">			<xsd:pattern value="S|N"/>		</xsd:restriction>	</xsd:simpleType>    <xsd:simpleType name="anno_type">        <xsd:restriction base="xsd:gYear">            <xsd:pattern value="\d{4}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="titolo_studio_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{8}"/>            <xsd:pattern value="NT"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="permesso_carta_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z0-9]{1,8}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="permesso_carta_motivo_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z]{3}|[A-Z]{4}[1-9]|[A-Z]{5}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="stato_occupazionale_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z]\d{0,3}"/>            <xsd:pattern value="NT"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="cpi_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{9}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="misura_type">        <xsd:restriction base="xsd:positiveInteger">            <xsd:pattern value="\d{1,2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="ateco_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{2}\.\d{2}\.\d{2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="mansioni_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[1-9]\.[1-9]\.[1-9]\.[1-9]\.\d\.\d{1,2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="tipo_contratto_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z]\.\d{2}\.\d{2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="modalita_lavoro_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z]{2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="soggetto_promotore_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="categoria_tirocinio_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{2}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="tipologia_tirocinio_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="[A-Z]"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="qualifica_srq_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\d{4}"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_type">        <xsd:restriction base="xsd:string">            <xsd:pattern value="\S[\S| ]*\S|\S"/>            <xsd:maxLength value= "512"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_1_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "1"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_3_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "3"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_5_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "5"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_8_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "8"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_15_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "15"/>        </xsd:restriction>    </xsd:simpleType>         <xsd:simpleType name="varchar_16_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "16"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_20_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "20"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_30_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "30"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_40_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "40"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_60_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "60"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_100_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "100"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_200_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "200"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_300_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "300"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="varchar_500_type">        <xsd:restriction base="tns:varchar_type">            <xsd:maxLength value= "500"/>        </xsd:restriction>    </xsd:simpleType>    <xsd:simpleType name="int_type">        <xsd:restriction base="xsd:positiveInteger">            <xsd:totalDigits value="11"/>        </xsd:restriction>    </xsd:simpleType></xsd:schema>