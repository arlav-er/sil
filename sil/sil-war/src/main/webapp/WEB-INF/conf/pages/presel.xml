<!-- Gestione Conoscenze Informatiche -->

<PAGE name="ConoscenzeInfoPage" scope="REQUEST">

  <MODULES>
    <MODULE name="M_ListConoscenzeInfo"/>
    <MODULE name="M_LoadConoscenzaInfo"/>
    <MODULE name="M_LoadLastConoscenzaInfo"/>
    <MODULE name="M_DeleteConoscenzaInfo"/>
    <MODULE name="M_InsertConoscenzaInfo"/>
    <MODULE name="M_UpdateConoscenzaInfo"/>
		<!-- Moduli di lista di supporto -->
    <MODULE name="M_ListTipiConoscenzaInfo"/>
    <MODULE name="M_ListGradiConoscenzaInfo"/>
    <MODULE name="M_ListModiConoscenzaInfo"/>
    <MODULE name="M_ListDettagliConoscenzaInfo"/>
    <MODULE name="M_LISTDETTAGLIALLCONOSCENZAINFO"/>
    
  </MODULES>

  <DEPENDENCIES>

    <!-- Carica sempre la lista dei tipi, 
         dei gradi e dei modi delle conoscenze info -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_ListTipiConoscenzaInfo">
      <CONDITIONS/>
    </DEPENDENCE>
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_ListGradiConoscenzaInfo">
      <CONDITIONS/>
    </DEPENDENCE>
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_ListModiConoscenzaInfo">
      <CONDITIONS/>
    </DEPENDENCE>
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_LISTDETTAGLIALLCONOSCENZAINFO">
      <CONDITIONS/>
    </DEPENDENCE>

    <!-- Per prima cosa carica la lista delle conoscenze -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_ListConoscenzeInfo">
      <CONDITIONS>
        <PARAMETER name="MODULE" scope="SERVICE_REQUEST" value="AF_NOT_DEFINED"/> 
      </CONDITIONS>
    </DEPENDENCE>

    <!-- Elimina una conoscenza e ricarica la lista -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_DeleteConoscenzaInfo">
      <CONDITIONS>
        <PARAMETER name="MODULE" scope="SERVICE_REQUEST" value="M_DeleteConoscenzaInfo"/> 
      </CONDITIONS>
    </DEPENDENCE>
    <DEPENDENCE source="M_DeleteConoscenzaInfo" target="M_ListConoscenzeInfo">
      <CONDITIONS/>
    </DEPENDENCE>

    <!-- Nella mainPage si vuole inserire una nuova conoscenza,
         dopo l'inserimento, ricarico l'ultimo inserimento in modo 
         da visualizzarlo nel pannello "nuovo" 
         e aggiono la lista conoscenze -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_InsertConoscenzaInfo">
      <CONDITIONS>
        <PARAMETER name="MODULE" scope="SERVICE_REQUEST" value="M_InsertConoscenzaInfo"/> 
      </CONDITIONS>
    </DEPENDENCE>
    <DEPENDENCE source="M_InsertConoscenzaInfo" target="M_LoadLastConoscenzaInfo">
      <CONDITIONS/>
    </DEPENDENCE>
    <DEPENDENCE source="M_LoadLastConoscenzaInfo" target="M_ListConoscenzeInfo">
      <CONDITIONS/>
    </DEPENDENCE>

    <!-- Carica una specifica conoscenza per poterla vedere / modificare -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_LoadConoscenzaInfo">
      <CONDITIONS>
        <PARAMETER name="PRGINFO" scope="SERVICE_REQUEST" value="AF_DEFINED"/>
      </CONDITIONS>
    </DEPENDENCE>

  <DEPENDENCE source="M_LoadConoscenzaInfo" target="M_ListConoscenzeInfo">
      <CONDITIONS/>
    </DEPENDENCE>


    <!-- Carica l'ultima conoscenza inserita per poterla vedere / modificare -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_LoadLastConoscenzaInfo">
      <CONDITIONS>
        <PARAMETER name="MODULE" scope="SERVICE_REQUEST" value="M_LoadLastConoscenzaInfo"/> 
      </CONDITIONS>
    </DEPENDENCE>

    <!-- Dalla pagina di dettaglio esegue un update dei dati 
         e torna alla mainPage -->
    <DEPENDENCE source="ConoscenzeInfoPage" target="M_UpdateConoscenzaInfo">
      <CONDITIONS>
        <PARAMETER name="MODULE" scope="SERVICE_REQUEST" value="M_UpdateConoscenzaInfo"/> 
      </CONDITIONS>
    </DEPENDENCE>
    <DEPENDENCE source="M_UpdateConoscenzaInfo" target="M_ListConoscenzeInfo">
      <CONDITIONS/>
    </DEPENDENCE>

    <!-- Carica i dettagli delle conoscenze info corrispondenti ad un certo Tipo -->
	 <DEPENDENCE source="ConoscenzeInfoPage" target="M_ListDettagliConoscenzaInfo">
     <CONDITIONS>
       <PARAMETER name="MODULE" scope="SERVICE_REQUEST" value="M_ListDettagliConoscenzaInfo"/> 
     </CONDITIONS>
   </DEPENDENCE>
  
  </DEPENDENCIES>

</PAGE>

