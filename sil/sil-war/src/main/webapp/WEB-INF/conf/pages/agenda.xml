				<PARAMETER name="PRGAPPUNTAMENTO" type="RELATIVE" scope="SERVICE_RESPONSE" value="MSalvaAppuntamento.PRGAPPUNTAMENTO" />
			</CONSEQUENCES>
		</DEPENDENCE>
		<!--RAMO DI CANCELLAZIONE LAVORATORE-->
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="DELETE_LAVORATORE_APPUNTAMENTO_AGENDA_MOD">
			<CONDITIONS>
				<PARAMETER name="AZIONE_DEL" scope="SERVICE_REQUEST" value="DEL" />
			</CONDITIONS>
		</DEPENDENCE>
		<DEPENDENCE source="DELETE_LAVORATORE_APPUNTAMENTO_AGENDA_MOD" target="SELECT_DETTAGLIO_AGENDA_MOD" />
