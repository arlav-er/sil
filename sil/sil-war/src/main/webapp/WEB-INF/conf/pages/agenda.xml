		<!-- Eliminato il 31/08/2004
			<DEPENDENCE source="SELECT_AGENDA_PAGE" target="MDelPattoApp">
			<CONDITIONS>
			<PARAMETER name="AZIONE_DEL" scope="SERVICE_REQUEST" value="DELPATTO"/>
			</CONDITIONS>
			</DEPENDENCE>
			<DEPENDENCE source="MDelPattoApp" target="SELECT_DETTAGLIO_AGENDA_MOD"/>
		-->

		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="M_DeleteAmLavPattoScelta">
			<CONDITIONS>
				<PARAMETER name="AZIONE_DEL" scope="SERVICE_REQUEST" value="DELPATTO" />
			</CONDITIONS>
		</DEPENDENCE>
		<DEPENDENCE source="M_DeleteAmLavPattoScelta" target="SELECT_DETTAGLIO_AGENDA_MOD" />
		<!--DEPENDENCE source="MSalvaAppuntamento" target="MAllineaSlot"/-->
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="MAllineaSlot">
			<CONDITIONS>
				<PARAMETER name="ALLINEA_SLOT" scope="SERVICE_REQUEST" value="AF_DEFINED" />
			</CONDITIONS>
			<CONSEQUENCES />
		</DEPENDENCE>

		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="SELECT_DETTAGLIO_AGENDA_MOD" />
		<DEPENDENCE source="SELECT_DETTAGLIO_AGENDA_MOD" target="MIncongruenze">
			<CONDITIONS>
				<PARAMETER name="SALVA" scope="SERVICE_REQUEST" value="AF_NOT_DEFINED" />
			</CONDITIONS>
		</DEPENDENCE>

		<DEPENDENCE source="SELECT_DETTAGLIO_AGENDA_MOD" target="LISTA_LAVORATORI_APPUNTAMENTO_AGENDA_MOD" />
		<DEPENDENCE source="SELECT_DETTAGLIO_AGENDA_MOD" target="MDati_Slot_App" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_SERVIZIO_SCAD" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_SPI_SCAD" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_SPI_SCAD_EFF" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_TIPO_PRENOTAZIONE" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_EFFETTO_APPUNTAMENTO" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_ESITO_APPUNTAMENTO" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_STATO_APPUNTAMENTO" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="COMBO_AMBIENTE" />
		<!--DEPENDENCE source="SELECT_AGENDA_PAGE" target="LISTA_LAVORATORI_APPUNTAMENTO_AGENDA_MOD"/-->
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="M_InvioEsitoAppuntamentoANPAL">
			<CONDITIONS>
				<PARAMETER name="ANPAL" scope="SERVICE_REQUEST" value="INVIOANPAL" />
			</CONDITIONS>
			<CONSEQUENCES />
		</DEPENDENCE>
		<DEPENDENCE source="M_InvioEsitoAppuntamentoANPAL" target="SELECT_DETTAGLIO_AGENDA_MOD" />
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="GetAppUltimoInvioAnpal" />
	</DEPENDENCIES>
</PAGE>

