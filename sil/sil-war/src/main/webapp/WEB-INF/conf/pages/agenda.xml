 	</MODULES>
	<DEPENDENCIES>
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="M_CONFIG_UMB_NGE_AZ"></DEPENDENCE>
		<DEPENDENCE source="SELECT_AGENDA_PAGE" target="MSalvaAppuntamento">
			<CONDITIONS>
				<PARAMETER name="SALVA" scope="SERVICE_REQUEST" value="AF_DEFINED" />
