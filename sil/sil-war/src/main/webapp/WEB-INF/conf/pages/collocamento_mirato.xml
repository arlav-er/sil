		</DEPENDENCE>

		<DEPENDENCE source="CM_INS_CONVENZIONE" target="CM_DETT_CONVENZIONI">
			<CONDITIONS>
				<PARAMETER name="inserisci" scope="SERVICE_REQUEST" value="AF_DEFINED"/>	
