			<PARAMETER type="RELATIVE" name="PRGINCROCIO" scope="SERVICE_RESPONSE" value="CMRIAPRIGRADUATORIA.ROW.PRGINCROCIO"/>
    	</CONSEQUENCES>
	</DEPENDENCE>	
