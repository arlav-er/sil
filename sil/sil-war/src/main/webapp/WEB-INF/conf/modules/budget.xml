                    <PARAMETER name="NUMANNOBUDGET" type="RELATIVE" value="NUMANNOBUDGET" scope="LOCAL"/>
                    <PARAMETER name="codcpi" type="RELATIVE" value="codcpi" scope="LOCAL"/>
