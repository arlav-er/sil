     <QUERIES>
	    <SELECT_QUERY statement="GET_AZIONI_MODELLO_TDA">
   	      <PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="PRGMODVOUCHER"/>
	    </SELECT_QUERY>
    </QUERIES>
    <COLUMNS>
    	 <COLUMN name="CODVCHATTIVITA" label="Codice"/>
    	 <COLUMN name="STRDESCRIZIONE" label="Descrizione"/>
     </COLUMNS>
    <CAPTIONS>
	<DELETE_CAPTION image="../../img/del.gif" label="Dissocia Azione" confirm="TRUE">
		<PARAMETER name="PAGE" type="ABSOLUTE" value="LinguettaAttivitaTdaPage" scope=""/>
        <PARAMETER name="OPERATION" type="ABSOLUTE" value="CANC" scope=""/>
        <PARAMETER name="cdnfunzione" type="RELATIVE" value="cdnfunzione" scope="SERVICE_REQUEST" />
        <PARAMETER name="PRGMODVOUCHER" type="RELATIVE" value="PRGMODVOUCHER" scope="SERVICE_REQUEST" />
 		<PARAMETER name="CODVCHATTIVITA" type="RELATIVE" value="CODVCHATTIVITA" scope="LOCAL" />
 		<PARAMETER name="PRGAZIONI" type="RELATIVE" value="PRGAZIONI" scope="LOCAL" />
	</DELETE_CAPTION>
    </CAPTIONS>
  </CONFIG>
</MODULE>
