<MODULE name="MListaTemplate" class="it.eng.afExt.dispatching.module.impl.DynamicStatementListModule">
  <CONFIG pool="SIL_DATI" title="Lista Template" rows="10">
	<QUERIES>
	   <SELECT_QUERY>
			<STATEMENT_PROVIDER CLASS="it.eng.sil.module.trento.TemplateList" />
	   </SELECT_QUERY>
	</QUERIES>
    <COLUMNS>
	  <COLUMN name="STRNOME" 				 label="Nome Template"/>
	  <COLUMN name="NOMECLASSIF" 				 label="Classificazione"/>
	  <COLUMN name="STRDESCRIZIONE" 		 label="Tipo Documento"/>
      <COLUMN name="STRNOTE" 	             label="Note"/>
    </COLUMNS>
    <CAPTIONS>
      <SELECT_CAPTION image="../../img/detail.gif" label="consulta o modifica" confirm="false">
        <PARAMETER name="PAGE"           type="ABSOLUTE" value="DettaglioTemplatePage" scope=""/>
        <PARAMETER name="MODULE"         type="ABSOLUTE" value="MDettaglioTemplate" scope=""/>
        <PARAMETER name="MOSTRATEMPLATE" type="ABSOLUTE" value="S"  	scope=""/>
        <PARAMETER name="PRGTEMPLATESTAMPA"  type="RELATIVE" value="PRGTEMPLATESTAMPA"  scope="LOCAL"/>
        <PARAMETER name="PRGCONFIGPROT"  type="RELATIVE" value="PRGCONFIGPROT"  scope="LOCAL"/>
        <PARAMETER name="CDNFUNZIONE"     type="RELATIVE" value="CDNFUNZIONE"     scope="SERVICE_REQUEST"/>
        <PARAMETER name="CODTIPODOMINIO"  type="RELATIVE" value="CODTIPODOMINIO" scope="SERVICE_REQUEST"/>
        <PARAMETER name="PRGCLASSIF"  type="RELATIVE" value="PRGCLASSIF" scope="LOCAL"/>
      </SELECT_CAPTION>
      <DELETE_CAPTION image="../../img/del.gif" label="Cancella template" confirm="TRUE">
  		<PARAMETER name="PAGE"           type="ABSOLUTE" value="GestTemplatePage" scope=""/>
        <PARAMETER name="MODULE"         type="ABSOLUTE" value="MDeleteTemplate" scope=""/>
        <PARAMETER name="PRGTEMPLATESTAMPA"  type="RELATIVE" value="PRGTEMPLATESTAMPA"  scope="LOCAL"/>
        <PARAMETER name="NUMKLOTEMP"  type="RELATIVE" value="NUMKLOTEMP"  scope="LOCAL"/>
        <PARAMETER name="CDNFUNZIONE"     type="RELATIVE" value="CDNFUNZIONE"     scope="SERVICE_REQUEST"/>
      </DELETE_CAPTION>
      <CAPTION image="../../img/Reitera.gif" label="Duplica template" confirm="FALSE">
  		<PARAMETER name="PAGE"           type="ABSOLUTE" value="DettaglioTemplatePage" scope=""/>
        <PARAMETER name="MODULE"         type="ABSOLUTE" value="MDettaglioTemplate" scope=""/>
        <PARAMETER name="DUPLICATEMPLATE" type="ABSOLUTE" value="S" scope=""/>
        <PARAMETER name="MOSTRATEMPLATE" type="ABSOLUTE" value="S" scope=""/>
        <PARAMETER name="PRGTEMPLATESTAMPA"  type="RELATIVE" value="PRGTEMPLATESTAMPA"  scope="LOCAL"/>
        <PARAMETER name="PRGCONFIGPROT"  type="RELATIVE" value="PRGCONFIGPROT"  scope="LOCAL"/>
        <PARAMETER name="CDNFUNZIONE"     type="RELATIVE" value="CDNFUNZIONE"     scope="SERVICE_REQUEST"/>
        <PARAMETER name="CODTIPODOMINIO"  type="RELATIVE" value="CODTIPODOMINIO" scope="SERVICE_REQUEST"/>
        <PARAMETER name="PRGCLASSIF"  type="RELATIVE" value="PRGCLASSIF" scope="LOCAL"/>
      </CAPTION>
    </CAPTIONS>
  </CONFIG>
</MODULE>

