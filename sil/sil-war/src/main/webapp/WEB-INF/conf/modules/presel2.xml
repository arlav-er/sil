			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="strArgTesi" />
			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="flgLode" />
			<PARAMETER scope="SESSION" type="RELATIVE" value="_CDUT_" />
			<PARAMETER scope="SESSION" type="RELATIVE" value="_CDUT_" />
		</QUERY_INSERT>
		<QUERY_SELECT statement="SELECT_UNIQUE_TITOLO_STUDIO">
			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="codTitolo" />
