				value="PRG_PATTO_LAVORATORE" />
			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="COD_LST_TAB" />
			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="PRG_TAB" />
