			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="PRGVALIDITA"/>
			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="CDNLAVORATORE" />
			<PARAMETER scope="SERVICE_REQUEST" type="RELATIVE" value="CODTIPOVALIDITA" />
