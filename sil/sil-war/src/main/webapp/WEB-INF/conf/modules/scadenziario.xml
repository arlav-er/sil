        <PARAMETER name="CDNLAVORATORE" type="RELATIVE" value="CDNLAVORATORE" scope="SERVICE_REQUEST"/>
      </DELETE_CAPTION>
      
    </CAPTIONS>    
  </CONFIG>
</MODULE>

